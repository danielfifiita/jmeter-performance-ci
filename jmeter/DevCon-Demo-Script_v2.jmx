        <stringProp name="parameters"></stringProp>
        <stringProp name="script">/* ********************************************************************************************** *
 * *****  USING Groovy to generate RANDOM variables could be useful and simpler to manage  ****** *
 * ********************** Moving the values to a CSV for inspection after *********************** *
 * ********************************************************************************************** */


import java.nio.file.*
import java.time.LocalDateTime
import java.time.format.DateTimeFormatter


/* Generate a random string (10 lowercase letters) */
def chars = (&apos;a&apos;..&apos;z&apos;).join()
def randomString  = (1..10).collect { chars[new Random().nextInt(chars.length())] }.join()

/* Generate a random number between 1 and 100 */
def randomNumber = (1..100).toList().get(new Random().nextInt(100))

/* Generate a random boolean */
def randomBoolean = new Random().nextBoolean()

/* Generate a random value from a predefined array/list */
def options = [&quot;apple&quot;, &quot;banana&quot;, &quot;cherry&quot;, &quot;mango&quot;, &quot;pear&quot;]
def randomArray = options[new Random().nextInt(options.size())]

/* UUID */
def uuid = java.util.UUID.randomUUID().toString()

/* Store them as JMeter variables (so you can use ${varName} later) */
vars.put(&quot;p_randomString&quot;, randomString)
vars.put(&quot;p_randomNumber&quot;, randomNumber.toString())
vars.put(&quot;p_randomBoolean&quot;, randomBoolean.toString())
vars.put(&quot;p_randomArray&quot;, randomArray)
vars.put(&quot;p_uuid&quot;, uuid)

/* Optional: print to log (for debugging) */
log.info(&quot;Generated values -&gt; randomString=${randomString}, randomNumber=${randomNumber}, randomBoolean=${randomBoolean}, randomArray=${randomArray}, randomUUID= ${uuid}&quot;)


/* ========== Write to CSV ========== */
def filePath = &quot;C:/DevCon/random_values_from_Jmeter.csv&quot;  // Relative to JMeter bin folder (or absolute path if preferred to store in specific location)
def file = new File(filePath)

/* Create the file with headers if it doesnâ€™t exist */
if (!file.exists()) {
    file.text = &quot;Timestamp,ThreadName,Iteration,randomString,randomNumber,randomBoolean,randomArray,uuid\n&quot;
}

/* Prepare line to append */
def timestamp = LocalDateTime.now().format(DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd HH:mm:ss&quot;))
def threadName = ctx.getThread().getThreadName()
def iteration = vars.getIteration()

def line = &quot;${timestamp},${threadName},${iteration},${randomString},${randomNumber},${randomBoolean},${randomArray},${uuid}\n&quot;

/* Append line to file */
Files.write(file.toPath(), line.getBytes(&quot;UTF-8&quot;), StandardOpenOption.APPEND)

/* ==========  Log for Debug ========== */
log.info(&quot;Generated random values -&gt; ${line.trim()}&quot;)

/* 
	Timestamp,ThreadName,Iteration,randomString,randomNumber,randomBoolean,randomArray,uuid
	2025-10-29 22:05:33,Thread Group 1-1,1,qwertyuiop,58,true,banana,a1f8-32df-...
	2025-10-29 22:05:35,Thread Group 1-1,2,zxcasdqwep,14,false,pear,fa23-2c2d-... 
	
*/

