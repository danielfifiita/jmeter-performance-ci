              <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(response)

// Fail if fewer than 10 posts returned
if (json.size() &lt; 10) {
    Failure = true
    FailureMessage = &quot;Expected at least 10 Posts/Users, but got: ${json.size()}&quot;
}

// Track violations
def titleIssues = []
def bodyIssues = []
def userIdIssues = []

json.each { post -&gt;
    if (post.title.length() &lt; 10) {
        titleIssues &lt;&lt; post.id
    }

    def lineBreaks = post.body.count(&apos;\n&apos;)
    if (lineBreaks &lt; 3) {
        bodyIssues &lt;&lt; post.id
    }

    if (!(1..10).contains(post.userId)) {
        userIdIssues &lt;&lt; post.id
    }
}

// Fail test if any violations found
if (!titleIssues.isEmpty() || !bodyIssues.isEmpty() || !userIdIssues.isEmpty()) {
    Failure = true
    FailureMessage = &quot;&quot;&quot;
    Title too short for post IDs: ${titleIssues}
    Body too short (less than 3 line breaks) for post IDs: ${bodyIssues}
    Invalid userId (not 1-10) for post IDs: ${userIdIssues}
    &quot;&quot;&quot;.stripIndent().trim()
}
</stringProp>
